    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.45",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "color": "^4.2.3",
    "eslint": "^8.21.0",
    "pinia": "^2.0.22",
    "postcss": "^8.4.16",
    "postcss-import": "^14.1.0",

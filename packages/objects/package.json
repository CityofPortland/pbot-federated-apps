{
  "name": "@pbotapps/objects",
  "version": "1.4.22",
  "description": "Very low-level objects shared by more than one service or app",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index",
  "typings": "dist/index",
  "exports": {
    ".": {

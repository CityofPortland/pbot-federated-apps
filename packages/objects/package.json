  "bugs": {
    "url": "https://github.com/CityofPortland/pbotapps-federated-apps/issues"
  },
  "homepage": "https://github.com/CityofPortland/pbotapps-federated-apps#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.6.3"
  }
